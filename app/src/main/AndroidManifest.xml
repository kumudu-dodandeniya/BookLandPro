<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobileapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileApp">
        <activity
            android:name=".HomeMainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Edit_book" />
        <activity android:name=".Add_Book" />
        <activity android:name=".View_Books" />
        <activity android:name=".BookView" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".CardOrders"
            android:exported="true"
            android:parentActivityName=".cartOrder" />
        <activity
            android:name=".cartOrder"
            android:exported="true"
            android:parentActivityName=".orders" />
        <activity
            android:name=".CashOnDelivery_Details"
            android:exported="true"
            android:parentActivityName=".orders" />
        <activity
            android:name=".orders"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>